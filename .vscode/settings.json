{
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "editor.formatOnSave": true,
  "files.associations": {
    "*.C": "c",
    "*.h": "c",
    "chrono": "c",
    "xlocale": "c",
    "algorithm": "c"
  },
  "c-cpp-flylint.language": "c",
  "c-cpp-flylint.clang.standard": ["c89", "c99", "c11", "c17", "c18"],
  "c-cpp-flylint.clang.extraArgs": ["-Wno-pragma-once-outside-header"],
  "c-cpp-flylint.cppcheck.standard": ["c89", "c99", "c11", "c17", "c18"]
}
